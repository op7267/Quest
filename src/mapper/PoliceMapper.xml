<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">  

<mapper namespace="dao.daoInterface">
	<insert id="insert_Police" parameterType="police">
		insert into police values(#{boardNo}, #{user_index})
	</insert>
	
	<select id="select_Police" parameterType="police" resultType="police"> 
		select * from police where boardNo=#{boardNo} and user_id=#{user_index}
	</select>

</mapper>